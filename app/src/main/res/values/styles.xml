<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="ComplexityTheme" parent="Theme.MaterialComponents.Bridge">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:navigationBarColor" tools:targetApi="lollipop">@color/colorPrimaryDark</item>
    </style>

    <style name="MainTheme" parent="Theme.MaterialComponents.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:navigationBarColor" tools:targetApi="lollipop">@color/colorPrimaryDark</item>
    </style>

    <style name="btnMain">
        <item name="android:layout_margin">2dp</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:paddingEnd">20dp</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:background">@drawable/btn_main_menu</item>
    </style>

</resources>